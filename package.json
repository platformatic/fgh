{
  "name": "fgh",
  "version": "1.0.0",
  "description": "A typescript implementation of the JQ language",
  "main": "dist/fgh.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "prepublishOnly": "rm -rf dist && tsc",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "node --no-warnings --experimental-strip-types --test"
  },
  "pre-commit": [
    "test",
    "lint",
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcollina/fgh.git"
  },
  "keywords": [
    "jq",
    "typescript",
    "json",
    "filter",
    "comma operator",
    "sequence"
  ],
  "author": "Matteo Collina <hello@matteocollina.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mcollina/fgh/issues"
  },
  "homepage": "https://github.com/mcollina/fgh#readme",
  "devDependencies": {
    "@fastify/pre-commit": "^2.2.0",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "neostandard": "^0.12.1"
  }
}
